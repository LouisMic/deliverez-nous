      <%= link_to 'Spectacles', dashboard_path, 'data-tab-name' => 'shows', class:'nav-link'%>
    </li>
    <li class="nav-item" data-dashboard-tabs-target="tab" data-tab-name="bookings">
      <a class="nav-link" href="#">RÃ©servations</a>
    </li>
  </ul>
  <div class="row my-3">
    <% if @tabs === 'seller' %>
      <%= render 'seller', my_shows: @my_shows %>
    <% else %>
      <%= render 'buyer', shows: @shows %>
    <% end %>
  </div>
</div>
